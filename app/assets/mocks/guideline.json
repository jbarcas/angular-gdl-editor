      "en": {
        "id": "en",
        "purpose": "To calculate estimated glomerular filtration rate based on serum creatine lab test result according to the Cockcroft-Gault formula , in males, females and children.",
        "keywords": [
          "estimated glomerular filtration rate",
          "estimated GFR",
          "kidney function",
          "creatinine",
          "kidney",
          "Cockcroft-Gault"
        ],
        "use": "Use for calculating eGFR based on Cockcroft-Gault formula.",
        "misuse": "Do not use for caulculating eGFR based on other formulas.",
        "copyright": "Cambio Healthcare Systems"
      },
      "da": {
        "id": "da",
        "copyright": "Cambio Healthcare Systems"
      },
      "es": {
        "id": "es",
        "purpose": "Calcular el índice de filtrado glomerular estimado basándose en el nivel de creatinina del sujeto",
